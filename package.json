{
  "name": "interactive-rating-component",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -i ./styles/global.css -o ./app/styles/global.css",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.{html,js}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@remix-run/node": "^1.6.0",
    "@remix-run/react": "^1.5.1",
    "@remix-run/vercel": "^1.5.1",
    "@vercel/node": "^1.15.4",
    "react": "^18.2.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.6.0",
    "@remix-run/serve": "^1.6.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "eslint": "^8.17.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "2.7.0",
    "tailwindcss": "^3.1.3",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": ">=14"
  }
